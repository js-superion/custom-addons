        <record id="group_mqc_merg" model="res.groups">
            <field name="name">急诊组</field>
            <field name="category_id" ref="base.module_category"/>
            <field name="implied_ids" eval="[(4,res('base.group_user))]"/>
        </record>

        <record id="group_mqc_merg_admin" model="res.groups">
            <field name="name">急诊组管理员</field>
            <field name="category_id" ref="base.module_category"/>
